{
  "expo": {
    "name": "TimerSim",
    "slug": "TimerSim",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/settingsIcon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/TimerSim.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.timersim.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptiveIcon.png",
        "backgroundImage": "./assets/adaptiveBack.png"
      },
      "package": "com.timersim.mobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
